                        <field name="hostile_area"/>
                    </group>
                    <newline/>
                    <newline/>
                    <group string="Notes" id="group_ses_notes">
                        <field colspan="4" name="ses_notes"/>
                    </group>
                </page>
                <page string="Family" id="page_family">
